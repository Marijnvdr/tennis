<h2>Results of seeded players:</h2>

<table class="colored-table">
  <thead>
    <tr>
      <th>Rank</th>
      <th>Player</th>
      <th>Round</th>
      <th></th>
      <th>Opponent</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |seeding|}}
      <tr>
        <td>{{seeding.seedingNr}}</td>
        <td>{{seeding.player.lastName}}</td>
        <td>4TH</td>
        <td class='info-font'>lost to:</td>
        <td>{{seeding.player.lastName}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

<div class="pagination">
  <ul>
    <li>{{#link-to "seeding" (query-params page="1")}}1{{/link-to}}</li>
    <li>{{#link-to "seeding" (query-params page="3")}}2{{/link-to}}</li>
  </ul>
</div>


